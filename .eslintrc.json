{
	"extends": [
		"plugin:react/recommended",
		"airbnb",
		"airbnb-typescript",
		"prettier"
	],
	"parserOptions": {
		"project": "./tsconfig.json"
	},
	"rules": {
		"react/jsx-props-no-spreading": "off",
		"react/react-in-jsx-scope": "off",
		"react/jsx-uses-react": "off",
		"react/jsx-filename-extension": [
			1,
			{ "extensions": [".js", ".jsx", ".ts", ".tsx"] }
		]
	}
}
